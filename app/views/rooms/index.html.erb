<div class="container">
  <div class="row">
    <div class="col-md-6 center-block">
      <p id="notice"><%= notice %></p>

      <h1>Rooms</h1>

      <ul class="mt-4 list-unstyled">
        <%= render @rooms %>
      </ul>

      <div class="mt-2">
        <%= render 'rooms/form', room: @room %>
      </div>
    </div>

    <div class="col-md-6 center-block">
      <p></p>
      <h1>Online User (<%= User.where(online: true).count %>)</h1>
      <div id="online" class="mt-4 list-unstyled">
        <%= render partial: 'users/user', collection: @users %>
      </div>
    </div>
  </div>
</div>